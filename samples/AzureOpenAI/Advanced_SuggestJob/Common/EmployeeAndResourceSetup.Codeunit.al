namespace CopilotToolkitDemo.Common;

using Microsoft.HumanResources.Employee;
using Microsoft.HumanResources.Setup;
using System.Text;
using System.Utilities;
using Microsoft.Service.Setup;
using Microsoft.Service.Resources;
using Microsoft.Projects.Resources.Resource;

codeunit 54325 "Employee And Resource Setup"
{
    Subtype = Install;
    Access = Internal;

    trigger OnInstallAppPerCompany()
    begin
        CreateDemoData();
    end;

    internal procedure CreateDemoData()
    begin
        EnsureResourcesCreatedAutomatically();

        CreateJannikEmployeeAndResource();
        CreateMikeEmployeeAndResource();
    end;

    local procedure CreateMikeEmployeeAndResource()
    var
        Employee: Record Employee;
        Resource: Record Resource;
        EmployeeResourceUpdate: Codeunit "Employee/Resource Update";
    begin
        Employee.SetRange("First Name", 'Mike');
        if not Employee.IsEmpty() then
            exit;

        Employee.SetRange("First Name");

        Employee.Validate("No.", 'Mike');
        Employee.Validate("First Name", 'Mike');
        Employee.Validate("Last Name", 'Morton');
        AddCommonEmployeeValues(Employee);
        AddPictureToEmployee(Employee, MikePic());
        Employee.Insert(true);

        // Resource
        EmployeeResourceUpdate.ResUpdate(Employee);
        Resource.Get(Employee."No.");
        AddCommonResourceValues(Resource);
        AddPictureToResource(Resource, MikePic());
        Resource.Modify(true);

        CreateAllResourceSkills(Resource."No.");
        AddResourceCapacity(Resource);
    end;

    local procedure CreateJannikEmployeeAndResource()
    var
        Employee: Record Employee;
        Resource: Record Resource;
        EmployeeResourceUpdate: Codeunit "Employee/Resource Update";
    begin
        Employee.SetRange("First Name", 'Jannik');
        if not Employee.IsEmpty() then
            exit;

        Employee.SetRange("First Name");

        Employee.Validate("No.", 'Jannik');
        Employee.Validate("First Name", 'Jannik');
        Employee.Validate("Last Name", 'Bausager');
        AddCommonEmployeeValues(Employee);
        AddPictureToEmployee(Employee, JannikPic());
        Employee.Insert(true);

        // Resource
        EmployeeResourceUpdate.ResUpdate(Employee);
        Resource.Get(Employee."No.");
        AddCommonResourceValues(Resource);
        AddPictureToResource(Resource, JannikPic());
        Resource.Modify(true);

        CreateAllResourceSkills(Resource."No.");
    end;

    local procedure AddCommonEmployeeValues(var Employee: Record Employee)
    begin
        Employee.Validate("Country/Region Code", 'DK');
        Employee.Validate(Address, 'Kanalvej, 7');
        Employee.Validate("Post Code", '2800');
        Employee.Validate(City, 'Kongens Lyngby');
    end;

    local procedure AddResourceCapacity(var Resource: Record Resource)
    var
        I: Integer;
        Day: Date;
    begin
        for I := -100 to 100 do begin
            Day := CalcDate(
                    StrSubstNo('%1D', Format(I)),
                    DT2Date(CurrentDateTime())
                );
            CreateResCapacity(Resource."No.", Day, 8.0);
        end;

        // Just in case...
        for I := -100 to 100 do begin
            Day := CalcDate(
                    StrSubstNo('%1D', Format(I)),
                    WorkDate()
                );
            CreateResCapacity(Resource."No.", Day, 8.0);
        end;
    end;

    local procedure CreateAllResourceSkills(ResourceNo: Code[20])
    begin
        CreateResourceSkill(ResourceNo, 'elec', 'Electric wiring');
        CreateResourceSkill(ResourceNo, 'techn', 'Technician');
        CreateResourceSkill(ResourceNo, 'ID', 'Interior design');
        CreateResourceSkill(ResourceNo, 'srv', 'Surveyor');
        CreateResourceSkill(ResourceNo, 'srvmgr', 'Service Manager');
    end;

    local procedure CreateResourceSkill(ResourceNo: Code[20]; SkillCodeCode: Code[10]; SkillCodeDescription: Text[100])
    var
        SkillCode: Record "Skill Code";
        ResourceSkill: Record "Resource Skill";
    begin
        if SkillCode.Get(SkillCodeCode) then SkillCode.Delete();

        SkillCode.Code := SkillCodeCode;
        SkillCode.Description := SkillCodeDescription;
        SkillCode.Insert();

        ResourceSkill.Init();
        ResourceSkill."No." := ResourceNo;
        ResourceSkill."Skill Code" := SkillCodeCode;
        ResourceSkill.Insert();
    end;

    local procedure CreateResCapacity(ResourceNo: Code[20]; Date: Date; Capacity: Decimal)
    var
        ResCapacityEntry: Record "Res. Capacity Entry";
        EntryNo: Integer;
    begin
        if ResCapacityEntry.FindLast() then;
        EntryNo := ResCapacityEntry."Entry No." + 1;
        ResCapacityEntry.Init();
        ResCapacityEntry."Entry No." := EntryNo;
        ResCapacityEntry."Resource No." := ResourceNo;
        ResCapacityEntry.Date := Date;
        ResCapacityEntry.Capacity := Capacity;
        ResCapacityEntry.Insert();
    end;

    local procedure AddCommonResourceValues(var Resource: Record Resource)
    begin
        Resource.Validate("Gen. Prod. Posting Group", 'SERVICES');
        // Resource.Validate("VAT Prod. Posting Group", 'VAT10');
        Resource.Validate("Indirect Cost %", 10);
        Resource.Validate("Direct Unit Cost", 42);
        Resource.Validate("Unit Price", 110);
        Resource.Validate("Base Unit of Measure", 'HOUR');
    end;

    local procedure AddPictureToEmployee(var Employee: Record Employee; Pic: Text)
    var
        InStr: InStream;
        TempBlob: Codeunit "Temp Blob";
        Base64Convert: Codeunit "Base64 Convert";
        OutStr: OutStream;
    begin
        TempBlob.CreateOutStream(OutStr);
        Base64Convert.FromBase64(Pic, Outstr);

        TempBlob.CreateInStream(InStr);
        Employee.Image.ImportStream(InStr, StrSubstNo('A picture of Employee %1.', Employee.FullName()));
    end;

    local procedure AddPictureToResource(var Resource: Record Resource; Pic: Text)
    var
        InStr: InStream;
        TempBlob: Codeunit "Temp Blob";
        Base64Convert: Codeunit "Base64 Convert";
        OutStr: OutStream;
    begin
        TempBlob.CreateOutStream(OutStr);
        Base64Convert.FromBase64(Pic, Outstr);

        TempBlob.CreateInStream(InStr);
        Resource.Image.ImportStream(InStr, StrSubstNo('A picture of Resource %1.', Resource.Name));
    end;

    local procedure EnsureResourcesCreatedAutomatically()
    var
        HumanResourcesSetup: Record "Human Resources Setup";
    begin
        if HumanResourcesSetup.Get() then begin
            HumanResourcesSetup."Automatically Create Resource" := true;
            HumanResourcesSetup.Modify();
        end;
    end;

    local procedure JannikPic() b64pic: Text
    begin
        b64pic += '';
        b64pic += '';
        b64pic += '';
    end;

    local procedure MikePic() b64pic: Text
    begin
        b64pic += '';
        b64pic += '';
        b64pic += '';
    end;

}